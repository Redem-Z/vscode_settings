{
    "env": {
        "myKernelSrcPath": "${workspaceFolder}/kernel"
    },
    "configurations": [
        {
            "name": "null",
            "includePath": [
                "${workspaceFolder}/**"
            ]
        },
        {
            "name": "Linux",
            "includePath": [
                "${myKernelSrcPath}/arch/arm64/**",
                "${myKernelSrcPath}/arch/arm64/include/**",
                "${myKernelSrcPath}/include/**",
                "${myKernelSrcPath}/include/linux/**",
                "${myKernelSrcPath}/**"
            ],
            "forcedInclude": [
                "${myKernelSrcPath}/include/generated/autoconf.h"
            ],
            "defines": [],
            "compilerPath": "/usr/bin/aarch64-linux-gnu-gcc",
            "cStandard": "c17",
            "cppStandard": "c++14",
            "intelliSenseMode": "gcc-arm64",
            "compileCommands": "${workspaceFolder}/.vscode/compile_commands.json",
            "browse": {
                "path": [
                    "${myKernelSrcPath}"
                ],
                "limitSymbolsToIncludedHeaders": true
            }
        }
    ],
    "version": 4
}